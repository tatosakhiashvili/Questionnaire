<!DOCTYPE html>
<html>
<head>
	<meta charset="utf-8" />
	<meta name="viewport" content="width=device-width, initial-scale=1.0">
	<title>@ViewBag.Title</title>
	@Styles.Render("~/Content/css")
	@*<link href="~/Content/framework/datetimePicker/bootstrap-datetimepicker.min.css" rel="stylesheet" />*@
	@*@Scripts.Render("~/bundles/modernizr")*@
	<link href="https://cdn.rawgit.com/Eonasdan/bootstrap-datetimepicker/a549aa8780dbda16f6cff545aeabc3d71073911e/build/css/bootstrap-datetimepicker.css" rel="stylesheet" />

	<link href="~/Content/framework/tableSearch/tableSearch.css" rel="stylesheet" />

</head>
<body>
	<div class="alert alert-danger fade in general-error-container">
		<a class="close">&times;</a>
		<strong class="error-title"></strong>
		<ul class="error-container">
			<li><span class="error-content">123</span></li>
		</ul>
	</div>

	<div class="alert alert-success fade in general-success-container">
		<a class="close">&times;</a>
		<strong class="success-title"></strong>
		<ul class="success-container">
			<li><span class="success-content"></span></li>
		</ul>
	</div>

	<div class="loader-container">
		<div class="line-loader"></div>
		<div class="loader-fade"></div>
	</div>

	@{ Html.RenderAction("Header", "Layout"); }

	@RenderBody()

	<div id="modal-container"></div>

	@Scripts.Render("~/bundles/jquery")
	@Scripts.Render("~/bundles/bootstrap")

	<script src="~/Scripts/framework/jqueryMask/jquery.mask.min.js"></script>
	<script src="~/Scripts/framework/treeView/bootstrap-treeview.js"></script>
	<script src="~/Scripts/moment.js"></script>
	<script src="~/Scripts/bootstrap-datetimepicker.js"></script>
	<script src="~/Scripts/framework/touchSpin/jquery.bootstrap-touchspin.js"></script>

	@Scripts.Render("~/custom/scripts")

	<script src="~/Scripts/main/questionnaire.js"></script>
	<script src="~/Scripts/main/questionnaire.utils.js"></script>
	<script src="~/Scripts/framework/uploadControl/uploadControl.js"></script>

	<script src="~/Scripts/pages/top/index.js"></script>

	<script src="~/Scripts/pages/chat/index.js"></script>

	<script src="~/Scripts/tinymce/tinymce.min.js"></script>

	<script src="~/Scripts/framework/highlighter/jquery.highlight.js"></script>

	<script src="~/Scripts/framework/jqueryTmpl/jQuery.tmpl.js"></script>

	<script src="~/Scripts/framework/tableSearch/searchableTable.js"></script>

	@RenderSection("scripts", required: false)

	@{ Html.RenderAction("CurrentMobileNumber", "Home"); }

</body>
</html>